{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore.Authentication": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "server-url": "http://localhost:8080",
    "realm": "woodvillage",
    "audience": "account",
    "name_claim": "preferred_username",
    "validate-issuer": false,
    "require-https": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=todo_db;Username=keycloak_db_user;Password=keycloak_db_user_password"
  }
}
